{
  "name": "@xmagic/stencil-less",
  "version": "3.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "prebuild": "rimraf ./dist && npm run test",
    "build": "tsc && npm run rollup",
    "watch": "tsc --watch",
    "rollup": "rollup -c rollup.config.mjs",
    "version": "npm run build",
    "release": "np",
    "test": "jest",
    "test.watch": "jest --watch"
  },
  "peerDependencies": {
    "@stencil/core": ">=2.0.0 || >=3.0.0-beta.0"
  },
  "devDependencies": {
    "less": "^4.1.3",
    "@types/less": "^3.0.3",
    "@ionic/prettier-config": "^2.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@stencil/core": "^3.0.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^16.11.48",
    "jest": "^26.6.3",
    "np": "^7.0.0",
    "prettier": "^2.2.1",
    "rimraf": "^5.0.0",
    "rollup": "^3.5.1",
    "terser": "^5.3.8",
    "ts-jest": "^26.4.4",
    "typescript": "^4.9.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/m310851010/stencil-less.git"
  },
  "author": "Ionic Team",
  "homepage": "https://stenciljs.com/",
  "description": "The Stencil Less Plugin",
  "keywords": [
    "stencil",
    "less"
  ],
  "jest": {
    "transform": {
      "^.+\\.(js|ts|tsx)$": "<rootDir>/test/jest.preprocessor.js"
    },
    "testRegex": "(\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ],
    "testURL": "http://localhost"
  }
}
